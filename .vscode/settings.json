{
    "[c]": {
        "editor.detectIndentation": false,
        "editor.tabSize": 8,
        "editor.insertSpaces": false,
        "editor.rulers": [80]
    },
    "C_Cpp.errorSquiggles": "Disabled",
    "files.exclude": {
        "**/.git": true,
        "**/*.cmd": true,
        "**/*.ko": true,
        "**/*.mod.c": true,
        "**/*.mod": true,
        "**/*.o": true,
        "Module.symvers": true,
        "modules.order": true
    },
    "cSpell.words": [
        "charp",
        "dmesg",
        "eniac",
        "insmod",
        "Kernelnewbies",
        "kernelspace",
        "Liran",
        "llseek",
        "loff",
        "mknod",
        "printk",
        "rmmod"
    ]
}
